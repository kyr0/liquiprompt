{
  "name": "liquiprompt",
  "type": "module",
  "version": "0.0.1",
  "description": "Smart prompt template language, optimizer and orchestrator",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "example": "tsx ./scripts/run-example.ts",
    "build": "pkgroll"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "author": "Aron Homberg <info@aron-homberg.de>",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^1.1.15",
    "@ai-sdk/openai": "^1.2.1",
    "@xenova/transformers": "^2.17.2",
    "ai": "^4.1.54",
    "cross-llm": "^0.1.3",
    "hnswlib-node": "^3.0.0",
    "json5": "^2.2.3",
    "liquidjs": "^10.15.0",
    "openai": "^4.86.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.0.8",
    "dotenv": "^16.4.5",
    "pkgroll": "^2.1.1",
    "prettyjson": "^1.2.5",
    "tsx": "^4.16.2",
    "typescript": "^5.0.0",
    "vitest": "^1.6.0"
  }
}
